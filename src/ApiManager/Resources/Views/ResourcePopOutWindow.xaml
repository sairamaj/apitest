		<GridSplitter Grid.Row="0" Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
		<TextBlock Grid.Row="0" Grid.Column="2" Text="content here" />
	</Grid>
</Window>
