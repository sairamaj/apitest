<Window x:Class="ApiManager.NewRequest.Views.TestHeaderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApiManager.NewRequest.Views"
        mc:Ignorable="d"
        Title="TestHeaderWindow" Height="650" Width="650">
	<Grid Margin="20">
		<Grid.RowDefinitions>
			<RowDefinition Height="30"/>
			<RowDefinition />
		</Grid.RowDefinitions>
		<Button Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right" Width="60" Content="Get" Command="{Binding GetCommand}" />
		<local:HeadersEditView  Grid.Row="1" Grid.Column="0"/>
	</Grid>
</Window>
